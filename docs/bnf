{
    "<methodannotation>": [ [ "<arguments>",  "NL", "<description>", "NL", "<result>" ] ],

    "<arguments>": [ [ "<argument>" ],
                     [ "<argument>", "NL", "<arguments>" ] ],

    "<argument>": [ [ "<linestart>", "@arg", "<typestatement>", "<name>", ":", "<comment>" ],
                    [ "<linestart>", "@arg", "<name>", ":", "<comment>" ] ],

    "<description>": [ [ "<chars>" ] ],


    "<result>": [ [ "<returnresult>" ],
                  [ "<asyncresult>" ],
                  [ "<chainresult>" ] ],

    "<returnresult>": [ [ "<linestart>", "@return", "<typestatement>", "<comment>" ],
                        [ "<linestart>", "@return", "<comment>" ] ],

    "<asyncresult>": [ [ "<asyncthenresult>" ],
                       [ "<asyncfailresult>" ] ],

    "<asyncthenresult>": [ [ "<linestart>" "@then" "<comment>" "<callback>" ] ],

    "<asyncfailresult>": [ [ "<linestart>" "@fail" "<asyncfailresultcases>" ] ],

    "<asyncfailresultcases>" :== "<asyncfailresultcase>"
                             | "<asyncfailresultcase>" NL "<asyncfailresultcases>" ],

    "<asyncfailresultcase>" :== "<linestart>" "<comment>" "<callback>" ],

    "<callback>": [  "(" "<callbackarguments>" ")" ],

    "<callbackarguments>": [  "<callbackargument>"
                          | "<callbackargument>", "<callbackarguments>" ],

    "<callbackargument>": [  "<typestatement>" "<comment>" ],

    "<chainresult>": [  "<linestart>" "@chain" ],

    "<typestatement>": [  ""<" "<types>" ">"" ],

    "<types>": [  "<type>"
              | "<type>" "|" "<types>" ],

    "<type>": [  string
             | number
             | object
             | array
             | boolean
             | [any-js-object]
             | [any-of-your-own-types] ],

    "<comment>": [  [any-string] ],

    "<name>": [  [valid-js-identifier] ],

    "<linestart>": [   NL "///" ],
